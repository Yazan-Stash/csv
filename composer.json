{
    "name": "yazanstash/csv",
    "description": "A nice API to write csv files.",
    "keywords": [
        "yazan",
        "stash",
        "csv"
    ],
    "homepage": "https://github.com/yazan-stash/csv",
    "license": "MIT",
    "authors": [
        {
            "name": "Yazan Stash",
            "email": "stash.yazan@gmail.com",
            "homepage": "https://ystash.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "YazanStash\\Csv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YazanStash\\Csv\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
